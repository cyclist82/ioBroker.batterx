{
  "name": "iobroker.batterx",
  "version": "0.0.1",
  "description": "Connect your batterX photo voltaik system",
  "author": {
    "name": "cyclist82",
    "email": "leiflampater@gmail.com"
  },
  "homepage": "https://github.com/cyclist82/ioBroker.batterx",
  "license": "MIT",
  "keywords": [
    "batterx",
    "power",
    "storage",
    "photo",
    "voltaik"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:cyclist82/ioBroker.batterx.git"
  },
  "engines": {
    "node": ">= 18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.6.8"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.6.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.6.0",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/adapter-react": "2.0.22",
    "@iobroker/testing": "^4.1.0",
    "@material-ui/core": "^4.12.4",
    "@tsconfig/node18": "^18.2.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.19",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^17.0.20",
    "@types/react": "^17.0.62",
    "@types/sinon": "^10.0.15",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.32.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.0.0",
    "proxyquire": "^2.1.3",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*|.eslintrc).{json,json5}",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "admin/build/",
    "build/",
    "www/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter all",
    "watch": "build-adapter all --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "prebuild:react": "rimraf admin/build",
    "build:react": "build-adapter react",
    "watch:react": "build-adapter react --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit && tsc --noEmit -p admin/tsconfig.json",
    "coverage": "nyc npm run test:ts",
    "lint": "eslint --ext .ts,.tsx src/ admin/src/",
    "translate": "translate-adapter",
    "release": "release-script"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bugs": {
    "url": "https://github.com/cyclist82/ioBroker.batterx/issues"
  },
  "readmeFilename": "README.md"
}